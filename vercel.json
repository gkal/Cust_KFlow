{
  "version": 2,
  "buildCommand": "pnpm run build",
  "installCommand": "pnpm install --no-frozen-lockfile",
  "framework": "vite",
  "devCommand": "pnpm run dev",
  "outputDirectory": "dist",
  "regions": ["fra1"],
  "github": {
    "silent": true
  },
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ]
} 